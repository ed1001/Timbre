<div class="user-media">
  <div class="user-vid">
    <iframe width="420" height="315" src="https://www.youtube.com/embed/<%= User.find(user).youtube_vid_id %>"></iframe>
  </div>
  <div class="user-sc">
    <iframe width="720" height="300" scrolling="no" frameborder="no" allow="autoplay" src="https://w.soundcloud.com/player/?url=<%= User.find(user).sc_url %>"></iframe>
  </div>
</div>
